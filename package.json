{
  "name": "pantry",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "build:cam-el": "wireit",
    "serve": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "vite",
      "dependencies": [
        "build:cam-el",
        "../../libs/ducks:build"
      ]
    },
    "build": {
      "command": "vite build",
      "dependencies": [
        "build:cam-el",
        "../../libs/ducks:build"
      ]
    },
    "build:cam-el": {
      "command": "cd ../../../lib/cam-el && pnpm i && pnpm build"
    },
    "serve": {
      "command": "vite preview"
    },
    "test": {
      "command": "vitest",
      "dependencies": [
        "../../libs/ducks:build"
      ]
    }
  },
  "devDependencies": {
    "@types/js-quantities": "^1.6.5",
    "@types/node": "^20.8.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "wireit": "^0.14.1"
  },
  "dependencies": {
    "@auzmartist/cam-el": "../../../lib/cam-el",
    "@auzmartist/ducks": "../../libs/ducks",
    "hybrids": "^8.2.5",
    "js-quantities": "^1.8.0",
    "lunr": "^2.3.9",
    "router-slot": "^1.5.5"
  }
}
